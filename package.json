{
  "name": "vue-ts-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:data": "tsx scripts/build-timeline-data.ts",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue src scripts",
    "lint:fix": "eslint --ext .js,.ts,.vue src scripts --fix",
    "format": "prettier --write '{src/**/*,scripts/**/*,*}.{js,ts,vue,json,md}'",
    "format:check": "prettier --check '{src/**/*,scripts/**/*,*}.{js,ts,vue,json,md}'",
    "typecheck": "vue-tsc --noEmit",
    "fetch:providers": "tsx scripts/fetch-provider-info.ts",
    "fetch:providers:dry-run": "tsx scripts/fetch-provider-info.ts --dry-run"
  },
  "license": "MIT",
  "description": "Minimal Vue + TypeScript starter with lint/format/typecheck.",
  "dependencies": {
    "cheerio": "^1.0.0",
    "markdown-it": "^14.1.0",
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.2.3",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "@types/cheerio": "^1.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "prettier": "^3.7.4",
    "tsx": "^4.20.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.1",
    "tailwindcss": "^3.4.17",
    "postcss": "^8.5.6",
    "autoprefixer": "^10.4.21"
  }
}

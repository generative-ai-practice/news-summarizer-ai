{
  "name": "vue-ts-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue src",
    "lint:fix": "eslint --ext .js,.ts,.vue src --fix",
    "format": "prettier --write '{src/**/*,*}.{js,ts,vue,json,md}'",
    "format:check": "prettier --check '{src/**/*,*}.{js,ts,vue,json,md}'",
    "typecheck": "vue-tsc --noEmit"
  },
  "license": "MIT",
  "description": "Minimal Vue + TypeScript starter with lint/format/typecheck.",
  "dependencies": {
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.1"
  }
}
